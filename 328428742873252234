local hour = os.date("*t").hour
local greetingText
if hour < 6 then
    greetingText = "Good night, "
elseif hour < 12 then
    greetingText = "Good morning, "
elseif hour < 19 then
    greetingText = "Good afternoon, "
else
    greetingText = "Good evening, "
end

