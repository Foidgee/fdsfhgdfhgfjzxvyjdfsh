-- Anti-insultos
local function filterBadWords(text)
    local filtered = text 
-- Reemplazos especiales definidos por el usuario
filtered = filtered:gsub("s", "ჽ")
filtered = filtered:gsub("S", "ჽ")
filtered = filtered:gsub("n", "ⴖ")
filtered = filtered:gsub("N", "ⴖ")
filtered = filtered:gsub("l", "I")
filtered = filtered:gsub("L", "I")
filtered = filtered:gsub("r", "ꞅ")
filtered = filtered:gsub("R", "ꞅ")
filtered = filtered:gsub("c", "ᲃ")
filtered = filtered:gsub("C", "ᲃ")
filtered = filtered:gsub("a", "ล")
filtered = filtered:gsub("A", "ล")

-- Reemplazo general del resto del abecedario con "〪"
filtered = filtered:gsub("B", "B〪")
filtered = filtered:gsub("b", "b〪")
filtered = filtered:gsub("D", "D〪")
filtered = filtered:gsub("d", "d〪")
filtered = filtered:gsub("E", "E〪")
filtered = filtered:gsub("e", "e〪")
filtered = filtered:gsub("F", "F〪")
filtered = filtered:gsub("f", "f〪")
filtered = filtered:gsub("G", "G〪")
filtered = filtered:gsub("g", "g〪")
filtered = filtered:gsub("H", "H〪")
filtered = filtered:gsub("h", "h〪")
filtered = filtered:gsub("I", "I〪")
filtered = filtered:gsub("i", "i〪")
filtered = filtered:gsub("J", "J〪")
filtered = filtered:gsub("j", "j〪")
filtered = filtered:gsub("K", "K〪")
filtered = filtered:gsub("k", "k〪")
filtered = filtered:gsub("M", "M〪")
filtered = filtered:gsub("m", "m〪")
filtered = filtered:gsub("O", "O〪")
filtered = filtered:gsub("o", "o〪")
filtered = filtered:gsub("P", "P〪")
filtered = filtered:gsub("p", "p〪")
filtered = filtered:gsub("Q", "Q〪")
filtered = filtered:gsub("q", "q〪")
filtered = filtered:gsub("T", "T〪")
filtered = filtered:gsub("t", "t〪")
filtered = filtered:gsub("U", "U〪")
filtered = filtered:gsub("u", "u〪")
filtered = filtered:gsub("V", "V〪")
filtered = filtered:gsub("v", "v〪")
filtered = filtered:gsub("W", "W〪")
filtered = filtered:gsub("w", "w〪")
filtered = filtered:gsub("X", "X〪")
filtered = filtered:gsub("x", "x〪")
filtered = filtered:gsub("Y", "Y〪")
filtered = filtered:gsub("y", "y〪")
filtered = filtered:gsub("Z", "Z〪")
filtered = filtered:gsub("z", "z〪")

    filtered = filtered:gsub(" ", "")
    return filtered
end

