-- Anti-insultos
local function filterBadWords(text)
    local filtered = text 
-- Reemplazos especiales definidos por el usuario
-- Letras especiales con símbolos y >
filtered = filtered:gsub("s", "s>")
filtered = filtered:gsub("S", "s>")
filtered = filtered:gsub("n", "n>")
filtered = filtered:gsub("N", "n>")
filtered = filtered:gsub("l", "I>")
filtered = filtered:gsub("L", "I>")
filtered = filtered:gsub("r", "r>")
filtered = filtered:gsub("R", "r>")
filtered = filtered:gsub("c", "ᲃ>")
filtered = filtered:gsub("C", "ᲃ>")
filtered = filtered:gsub("a", "a>")
filtered = filtered:gsub("A", "a>")

-- Resto del abecedario con >
filtered = filtered:gsub("b", "b>")
filtered = filtered:gsub("B", "B>")
filtered = filtered:gsub("d", "d>")
filtered = filtered:gsub("D", "D>")
filtered = filtered:gsub("e", "e>")
filtered = filtered:gsub("E", "E>")
filtered = filtered:gsub("f", "f>")
filtered = filtered:gsub("F", "F>")
filtered = filtered:gsub("g", "g>")
filtered = filtered:gsub("G", "G>")
filtered = filtered:gsub("h", "h>")
filtered = filtered:gsub("H", "H>")
filtered = filtered:gsub("i", "i>")
filtered = filtered:gsub("I", "I>")
filtered = filtered:gsub("j", "j>")
filtered = filtered:gsub("J", "J>")
filtered = filtered:gsub("k", "k>")
filtered = filtered:gsub("K", "K>")
filtered = filtered:gsub("m", "m>")
filtered = filtered:gsub("M", "M>")
filtered = filtered:gsub("o", "o>")
filtered = filtered:gsub("O", "O>")
filtered = filtered:gsub("p", "p>")
filtered = filtered:gsub("P", "P>")
filtered = filtered:gsub("q", "q>")
filtered = filtered:gsub("Q", "Q>")
filtered = filtered:gsub("t", "t>")
filtered = filtered:gsub("T", "T>")
filtered = filtered:gsub("u", "u>")
filtered = filtered:gsub("U", "U>")
filtered = filtered:gsub("v", "v>")
filtered = filtered:gsub("V", "V>")
filtered = filtered:gsub("w", "w>")
filtered = filtered:gsub("W", "W>")
filtered = filtered:gsub("x", "x>")
filtered = filtered:gsub("X", "X>")
filtered = filtered:gsub("y", "y>")
filtered = filtered:gsub("Y", "Y>")
filtered = filtered:gsub("z", "z>")
filtered = filtered:gsub("Z", "Z>")


    filtered = filtered:gsub(" ", "")
    return filtered
end

