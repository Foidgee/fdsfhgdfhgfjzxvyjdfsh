local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TextChatService = game:GetService("TextChatService")
local localPlayer = Players.LocalPlayer
local Clipboard = setclipboard or toclipboard or function(_) end

local loggingEnabled = false
local logParagraphs = {}
local logData = {}

local function addLog(user, msg)
    local paragraph = ChatTab:CreateParagraph({
        Title = user,
        Content = msg
    })
    table.insert(logParagraphs, paragraph)
    table.insert(logData, {user = user, msg = msg})
end

local function clearLogs()
    for _, paragraph in ipairs(logParagraphs) do
        if paragraph and paragraph.Remove then
            paragraph:Remove()
        elseif paragraph and paragraph.Destroy then
            paragraph:Destroy()
        end
    end
    logParagraphs = {}
    logData = {}
end

ChatTab:CreateToggle({
    Name = "Enable chat logging",
    CurrentValue = false,
    Flag = "logToggle",
    Callback = function(value)
        loggingEnabled = value
        if not loggingEnabled then
            clearLogs()
        end
    end,
})

ChatTab:CreateButton({
    Name = "Copy logs",
    Callback = function()
        if #logData == 0 then
            Rayfield:Notify({
                Title = "Error",
                Content = "No logs to copy.",
                Duration = 3,
                Image = 4483362458
            })
            return
        end

        local combinedText = ""
        for _, entry in ipairs(logData) do
            combinedText ..= entry.user .. " / " .. entry.msg .. "\n"
        end

        Clipboard(combinedText)

        Rayfield:Notify({
            Title = "Copied",
            Content = "Logs copied to clipboard.",
            Duration = 3,
            Image = 4483362458
        })
    end,
})

local function setupClassicChatListener()
    local chatEvents = ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
    if not chatEvents then
        return false
    end
    local onMessageFiltered = chatEvents:FindFirstChild("OnMessageDoneFiltering")
    if not onMessageFiltered then
        return false
    end

    onMessageFiltered.OnClientEvent:Connect(function(messageData)
        if not loggingEnabled then return end
        local user = messageData.SpeakerName or "Unknown"
        local msg = messageData.Message or ""
        if user == localPlayer.Name then return end
        addLog(user, msg)
    end)
    return true
end

local function setupModernChatListener()
    if TextChatService.ChatVersion ~= Enum.ChatVersion.TextChatService then
        return false
    end
    local textChannels = TextChatService:WaitForChild("TextChannels", 5)
    if not textChannels then
        return false
    end

    local function connectChannel(channel)
        if channel.ClassName == "TextChannel" then
            channel.MessageReceived:Connect(function(textMessage)
                if not loggingEnabled then return end
                local user = textMessage.TextSource.Name or "Unknown"
                local msg = textMessage.Text or ""
                if user == localPlayer.Name then return end
                addLog(user, msg)
            end)
        end
    end

    for _, channel in pairs(textChannels:GetChildren()) do
        connectChannel(channel)
    end

    textChannels.ChildAdded:Connect(connectChannel)

    return true
end

local successClassic = setupClassicChatListener()
local successModern = setupModernChatListener()

if not (successClassic or successModern) then
    ChatTab:CreateParagraph({
        Title = "Error",
        Content = "No compatible chat system detected."
    })
end



