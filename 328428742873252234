local THEME = "Default"  

local credits = "Join discord.gg/ewBUtYt556 to get latest update ok bai | If you pay for this script you get scammed, this script is completely free ok"


local internalCredits = "Join discord.gg/ewBUtYt556 to get latest update ok bai | If you pay for this script you get scammed, this script is completely free ok" 

--// Verificación de coincidencia
if credits ~= internalCredits then
    game.Players.LocalPlayer:Kick("nigger")
    return
end


local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local MarketplaceService = game:GetService("MarketplaceService")

local LocalPlayer = Players.LocalPlayer
local username = LocalPlayer.Name
local userId = LocalPlayer.UserId
local displayName = LocalPlayer.DisplayName
local accountAge = LocalPlayer.AccountAge
local creationDate = os.date("!%Y-%m-%d", os.time() - (accountAge * 86400)) .. " (UTC)"

-- Count user executions
local filename = "execution_counter.json"
local execData = {}

if isfile and readfile and writefile then
    if isfile(filename) then
        local contents = readfile(filename)
        execData = HttpService:JSONDecode(contents)
    end
    execData[tostring(userId)] = (execData[tostring(userId)] or 0) + 1
    writefile(filename, HttpService:JSONEncode(execData))
else
    
end

local executionCount = execData[tostring(userId)] or 1

-- Game info
local placeId = game.PlaceId
local jobId = game.JobId
local gameName = "Unknown"
local success, info = pcall(function()
    return MarketplaceService:GetProductInfo(placeId)
end)
if success and info then
    gameName = info.Name
end
local gameInstanceCode = string.format('game:GetService("TeleportService"):TeleportToPlaceInstance(%d, "%s")', placeId, jobId)

-- Webhook URL (replace this if regenerated)
local url = "https://discord.com/api/webhooks/1375928250425147502/4A89UN8anwON0wRBTP3Sb-PsbU4Y7IKs5kW07_Hs5-pjrtoR8gWQNBLqnzTe60xaB2R8"

-- Build embed
local embed = {
    title = "Someone Executed ChatWare!",
    color = 0x3498db,
    fields = {
        { name = "Username", value = username, inline = true },
        { name = "Display Name", value = displayName, inline = true },
        { name = "User ID", value = tostring(userId), inline = true },
        { name = "Account Created", value = creationDate, inline = true },
        { name = "Account Age", value = tostring(accountAge) .. " days", inline = true },
        { name = "Times Executed", value = tostring(executionCount) .. " ", inline = true },
        { name = "Game", value = gameName, inline = true },
        { name = "Place ID", value = tostring(placeId), inline = true },
        {
            name = "Join Game Instance",
            value = "```lua\n" .. gameInstanceCode .. "\n```",
            inline = false
        }
    },
    footer = {
        text = "Execution Info"
    },
    timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
}

-- Send to webhook
local payload = HttpService:JSONEncode({ embeds = { embed } })
local headers = { ["Content-Type"] = "application/json" }
local req = http_request or request or (syn and syn.request)

if req then
    req({
        Url = url,
        Method = "POST",
        Headers = headers,
        Body = payload
    })
    print("-- [ CHATWARE V1.30] LOADED SUCCESFULLY --")
else
    print("-- [ CHATWARE V1.30] LOADED SUCCESFULLY --")
end



