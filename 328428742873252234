local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local targetUsername = "eazye_726"

local function createBillboardTag(character)
    local head = character:WaitForChild("Head")

    local tag = Instance.new("BillboardGui")
    tag.Name = "CoolBillboard"
    tag.Parent = head
    tag.Size = UDim2.new(8, 0, 2, 0)
    tag.StudsOffset = Vector3.new(0, 2.7, 0)
    tag.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = tag
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.TextScaled = true
    label.Text = "ChatWare Dev"
    label.Font = Enum.Font.FredokaOne
    label.TextStrokeTransparency = 0.2
    label.TextStrokeColor3 = Color3.new(1, 1, 1)
    label.TextColor3 = Color3.fromRGB(255, 0, 0)

    -- Efecto arcoíris animado
    local uiStroke = Instance.new("UIStroke", label)
    uiStroke.Thickness = 2

    task.spawn(function()
        while tag.Parent do
            for hue = 0, 1, 0.01 do
                local color = Color3.fromHSV(hue, 1, 1)
                label.TextColor3 = color
                uiStroke.Color = color:Lerp(Color3.new(1, 1, 1), 0.4)
                task.wait(0.03)
            end
        end
    end)
end

local function monitorPlayer(player)
    player.CharacterAdded:Connect(function(char)
        task.wait(1)
        -- Solo lo crea si el jugador local NO es el objetivo
        if LocalPlayer.Name ~= targetUsername then
            createBillboardTag(char)
        end
    end)
    if player.Character then
        task.wait(1)
        if LocalPlayer.Name ~= targetUsername then
            createBillboardTag(player.Character)
        end
    end
end

-- Busca al jugador objetivo y lo monitorea
local targetPlayer = Players:FindFirstChild(targetUsername)
if targetPlayer then
    monitorPlayer(targetPlayer)
end

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local targetUsername = "narutosasuke2096"

local function createBillboardTag(character)
    local head = character:WaitForChild("Head")

    local tag = Instance.new("BillboardGui")
    tag.Name = "CoolBillboard"
    tag.Parent = head
    tag.Size = UDim2.new(8, 0, 2, 0)
    tag.StudsOffset = Vector3.new(0, 2.7, 0)
    tag.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Parent = tag
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.TextScaled = true
    label.Text = "ChatWare Dev"
    label.Font = Enum.Font.FredokaOne
    label.TextStrokeTransparency = 0.2
    label.TextStrokeColor3 = Color3.new(1, 1, 1)
    label.TextColor3 = Color3.fromRGB(255, 0, 0)

    -- Efecto arcoíris animado
    local uiStroke = Instance.new("UIStroke", label)
    uiStroke.Thickness = 2

    task.spawn(function()
        while tag.Parent do
            for hue = 0, 1, 0.01 do
                local color = Color3.fromHSV(hue, 1, 1)
                label.TextColor3 = color
                uiStroke.Color = color:Lerp(Color3.new(1, 1, 1), 0.4)
                task.wait(0.03)
            end
        end
    end)
end

local function monitorPlayer(player)
    player.CharacterAdded:Connect(function(char)
        task.wait(1)
        -- Solo lo crea si el jugador local NO es el objetivo
        if LocalPlayer.Name ~= targetUsername then
            createBillboardTag(char)
        end
    end)
    if player.Character then
        task.wait(1)
        if LocalPlayer.Name ~= targetUsername then
            createBillboardTag(player.Character)
        end
    end
end

-- Busca al jugador objetivo y lo monitorea
local targetPlayer = Players:FindFirstChild(targetUsername)
if targetPlayer then
    monitorPlayer(targetPlayer)
end
