-- Añadir botón en ChatClearer tab
ChatClearerTab:CreateButton({
    Name = "Clear Chat ",
    Callback = function()
        local TextChatService = game:GetService("TextChatService")
        if TextChatService.ChatVersion ~= Enum.ChatVersion.TextChatService then
            sendChatMessage("clearchat doesnt work on this type of chat.")
            return
        end

        local blob = "\u{000D}" -- Carácter especial (retorno de carro)
        local repeatedBlob = string.rep(blob .. " ", 20)
        local message = "." .. repeatedBlob .. "Chat Cleared!"
        sendChatMessage(message)
    end
})

